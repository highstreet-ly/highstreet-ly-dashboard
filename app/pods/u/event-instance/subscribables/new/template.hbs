{{page-title (concat "New subscribable for " this.eventInstance.name)}}
<div class="tab-content">
  <div
    class="tab-pane active show"
  >
    <div class="card">
      <div class="card-body">
        <h4 class="card-title">
          Add a new subscribable
        </h4>
        <Forms::SubscribableForm
          @plan={{this.plan}}
          @productsQuery={{this.productsQuery}}
          @searchQuery={{this.searchQuery}}
          @addProduct={{this.addProduct}}
          @removeProduct={{this.removeProduct}}
        />
      </div>
    </div>
  </div>
</div>
{{outlet}}